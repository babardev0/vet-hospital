{
  "version": 2,
  "builds": [
    {
      "src": "dist/vet-marketplace/server/main.js",
      "use": "@vercel/node"
    },
    {
      "src": "dist/vet-marketplace/browser/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/dist/vet-marketplace/browser/assets/$1"
    },
    {
      "src": "/(.*\\.(js|css|ico|png|jpg|jpeg|svg|json))",
      "dest": "/dist/vet-marketplace/browser/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/vet-marketplace/server/main.js"
    }
  ]
} 